   "source": [
    "
    "rheology_params_diff = RM.select_rheology_parameters(\"WH23_wet_diffusion\")\n",
    "rheology_params_disl = RM.select_rheology_parameters(\"WH23_wet_dislocation\")\n",
    "\n",
    "options = CreepOptions(\n",
    "    strain_rate=np.nan,\n",
    "    stress=50.0, # MPa\n",
    "    pressure=100e6, # Pa\n",
    "    temperature=1250 + 273.15, # K\n",
    "    grain_size=15.0, # mum\n",
    "    cOH=500.0 # H / 10^6 Si\n",
    ")\n",
    "strain_rate_diff_ref = RM.compute_strain_rate_creep(rheology_params_diff, options)\n",
    "strain_rate_disl_ref = RM.compute_strain_rate_creep(rheology_params_disl, options)\n",
    "\n",
    "rheology_params_variations = RM.set_trivial_rheology_parameters()\n",
    "rheology_params_variations = rheology_params_variations._replace(activation_energy=100e3)  # increase activation energy by 100 kj/mol\n",
    "\n",
    "# refit diffusion creep\n",
    "rheology_params_diff_new = RM.vary_rheology_parameters(rheology_params_diff, rheology_params_variations, options)\n",
    "print(\"rheology_params_diff_new: \", rheology_params_diff_new)\n",
    "\n",
    "# refit dislocation creep\n",
    "rheology_params_disl_new = RM.vary_rheology_parameters(rheology_params_disl, rheology_params_variations, options)\n",
    "print(\"rheology_params_disl_new: \", rheology_params_disl_new)\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "options_new = options._replace(strain_rate=strain_rate_diff_ref)\n",
    "\n",
    "vicosity_diff_old = RM.compute_viscosity_creep(rheology_params_diff, options_new)\n",
    "vicosity_diff = RM.compute_viscosity_creep(rheology_params_diff_new, options_new)\n",
    "\n",
    "print(\"vicosity_diff_old: \", vicosity_diff_old)\n",
    "print(\"vicosity_diff: \", vicosity_diff)\n",
    "\n",
    "vicosity_disl_old = RM.compute_viscosity_creep(rheology_params_disl, options_new)\n",
    "vicosity_disl = RM.compute_viscosity_creep(rheology_params_disl_new, options_new)\n",
    "\n",
    "print(\"vicosity_disl_old: \", vicosity_disl_old)\n",
    "print(\"vicosity_disl: \", vicosity_disl)"
   ]
